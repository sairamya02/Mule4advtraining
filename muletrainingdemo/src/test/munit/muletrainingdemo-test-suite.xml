		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="36823a1b-c389-450e-8ced-4e15baf8cef2" actual="#[payload]" expected='#["Welcome!Bruce Wayne.Your customer id is 100."]' message="the munit is success"/>
